{
  "name": "ro-crate-zip-explorer",
  "version": "0.3.1",
  "description": "A TypeScript library for browsing and extracting contents within RO-Crate zip archives.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "check-format": "prettier --check .",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "lint": "eslint .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "ci": "yarn build && yarn check-format && yarn lint && yarn check-exports && yarn test",
    "prepublishOnly": "yarn run ci",
    "local-release": "changeset version && changeset publish"
  },
  "keywords": [
    "RO-Crate",
    "zip",
    "explorer",
    "library",
    "TypeScript"
  ],
  "author": "davelopez",
  "license": "MIT",
  "repository": "davelopez/ro-crate-zip-explorer",
  "files": [
    "dist"
  ],
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.28.0",
    "@eslint/js": "^9.11.0",
    "@types/node": "^22.5.5",
    "@types/pako": "^2.0.3",
    "@vitest/coverage-v8": "2.1.1",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "happy-dom": "^15.7.4",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vitest": "2.1.1"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "pako": "^2.1.0",
    "ro-crate": "^3.4.6"
  }
}
